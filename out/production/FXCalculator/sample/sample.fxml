<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="440.0" prefWidth="410.0">
         <children>
            <TextField fx:id="txtVal1" layoutX="40.0" layoutY="102.0" />
            <TextField fx:id="txtVal2" layoutX="212.0" layoutY="102.0" />
            <Label layoutX="87.0" layoutY="69.0" text="Value 1" />
            <Label layoutX="273.0" layoutY="69.0" text="Value 2" />
            <Label layoutX="146.0" layoutY="22.0" text="Calculator">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="btnAdd" layoutX="61.0" layoutY="195.0" mnemonicParsing="false" onAction="#addition" text="ADD" />
            <Button fx:id="btnSub" layoutX="137.0" layoutY="195.0" mnemonicParsing="false" onAction="#substraction" text="SUB" />
            <Button fx:id="btnMul" layoutX="218.0" layoutY="195.0" mnemonicParsing="false" onAction="#multiplication" text="MUL" />
            <Button fx:id="btnDiv" layoutX="287.0" layoutY="195.0" mnemonicParsing="false" onAction="#division" text="DIV" />
            <Label fx:id="lblResult" layoutX="202.0" layoutY="278.0" />
            <Label layoutX="107.0" layoutY="278.0" text="Result:" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
